{
  "name": "@quark/docs",
  "version": "0.5.0",
  "description": "Documentation for quark",
  "main": "index.js",
  "scripts": {
    "release-stable": "cross-env RELEASE_TYPE=stable npm run prepare-release",
    "release-insiders": "cross-env RELEASE_TYPE=insiders npm run prepare-release",
    "release-all": "npm run release-stable && npm run release-insiders",
    "prepare-release": "ts-node scripts/download-raw-content.ts && ts-node scripts/electron-dependent.ts",
    "tags": "ts-node scripts/non-electron-dependent.ts",
    "google": "git checkout google && git merge master && git push && git checkout master",
    "start": "npm run tags && vuepress dev -p 80",
    "qs": "vuepress dev -p 80",
    "build": "npm run tags && vuepress build",
    "cloud-build": "npm run tags && vuepress build",
    "post-build": "npx ts-node ./scripts/create-site-map.ts && npm run worker",
    "worker": "npx ts-node ./scripts/generate-service-worker.ts",
    "deploy-ci": "firebase deploy --token \"$FIREBASE_TOKEN\"",
    "deploy-to-bucket": "ts-node ./scripts/cloud-storage-hosting.ts",
    "bad": "npm run build && firebase deploy",
    "test": "npm run build",
    "check": "ts-node ./scripts/check-files.ts",
    "cache": "ts-node ./scripts/purge-cache.ts",
    "server": "cd ./.vuepress/dist && http-server -p 80",
    "open": "ts-node scripts/open-links.ts",
    "vid": "ts-node ./scripts/convert-videos.ts",
    "ssn": "ts-node ./scripts/send-build-notification.ts",
    "loc": "bash -c 'cloc $(git ls-files)'"
  },
  "keywords": [
    "quark"
  ],
  "author": "Nishkal Kashyap",
  "license": "MIT",
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.17",
    "@ffprobe-installer/ffprobe": "^1.0.10",
    "@types/fluent-ffmpeg": "^2.1.9",
    "@types/fs-extra": "^5.0.5",
    "@types/open": "^6.1.0",
    "@types/package-json": "^5.0.1",
    "@types/recursive-readdir": "^2.2.0",
    "@types/sharp": "^0.22.2",
    "@types/single-line-log": "^1.1.0",
    "@types/table": "^4.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "firebase-tools": "^6.7.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-it-html5-embed": "^1.0.0",
    "material-icon-theme": "^3.7.1",
    "node-sass": "^7.0.0",
    "open": "^6.1.0",
    "recursive-readdir": "^2.2.2",
    "sass-loader": "^7.1.0",
    "sharp": "^0.22.1",
    "single-line-log": "^1.1.2",
    "svg-inline-loader": "^0.8.0",
    "table": "^5.2.3",
    "vuepress": "^0.14.11",
    "vuesax": "^3.8.35",
    "vuetify": "^1.4.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^2.5.0",
    "@ionic/core": "^4.1.2",
    "@ionic/vue": "0.0.2",
    "@slack/web-api": "^5.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/mock-require": "^2.0.0",
    "@types/node-fetch": "^2.1.7",
    "@types/yamljs": "^0.2.30",
    "animejs": "^3.0.1",
    "compare-versions": "^3.4.0",
    "dotenv": "^7.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase-functions": "^2.3.0",
    "js-beautify": "^1.9.0",
    "markdown-it-admonition": "^1.0.4",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-mermaid": "^0.2.5",
    "markdown-it-multimd-table": "^3.1.3",
    "markdown-it-task-lists": "^2.1.1",
    "mock-require": "^3.0.3",
    "node-fetch": "^2.3.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack-dev-middleware": "^3.6.0",
    "workbox-build": "^4.3.1",
    "yamljs": "^0.3.0"
  }
}
