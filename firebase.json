{
  "hosting": [{
    "public": "./.vuepress/dist",
    "target": "docs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/download-count/**",
        "function": "downloadCount"
      },
      {
        "source": "/crash-reporter/**",
        "function": "crashReporter"
      }
    ],
    "cleanUrls": false,
    "trailingSlash": false,
    "headers": [{
        "source": "**/*.@(jpg|jpeg|gif|png|mp4)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=1209600"
        }]
      },
      {
        "source": "**/!(*worker).js",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=1209600"
        }]
      }
    ]
  }]
}